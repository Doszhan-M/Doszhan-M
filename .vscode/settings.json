{
  // - - - Настройки liveSassCompile - - - - - - - - 
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/css"
    },
    {
      "format": "compressed",
      "extensionName": ".min.css",
      "savePath": "/src/css"
    },
  ],
  "liveSassCompile.settings.autoprefix": [
    "> 1%",
    "last 2 versions",
  ],
  "editor.snippetSuggestions": "top",
  // - - - Настройки cSpell - - - - - - - - - - - - -
  "cSpell.language": "en,ru",
  "cSpell.ignoreWords": [
    "mocerts_db",
    "Предоплаченные",
    "серт",
    "CHATID",
    "Doszhan",
    "QIWI",
    "signup",
    "токену",
    "ChocoTest",
    "todolist",
    "signinPage",
    "minmax",
    "todocreate",
    "todoadd__wrapper",
    "алерт",
    "slaider",
    "скрола",
  ],
  "cSpell.words": [
    "changefreq",
    "fanblog",
    "Gffa",
    "lastmod",
    "libx",
    "Mengaliyev",
    "MZSXTEP",
    "reportbot",
    "SLACKBOT",
    "superfast",
    "ultrafast",
    "vcodec",
    "veryslow",
    "бана"
  ],
  "liveServer.settings.port": 5501,
}